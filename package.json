{
  "name": "dlect",
  "version": "0.3.1",
  "description": "Avoid React avoidable re-renders with Redux, Reselect and Normalizr using an optimisied selector.",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test build",
    "pretest": "npm run build",
    "prepare": "npm run clean && npm test && npm run build",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "test:ci": "jest --coverage --ci --testResultsProcessor=jest-junit --coverageReporters=text-lcov | coveralls",
    "clean": "rimraf lib dist es coverage"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "index.d.ts"
  ],
  "keywords": [
    "normalizr",
    "reselect",
    "react",
    "redux",
    "selector",
    "denormalize",
    "normalize"
  ],
  "repository": "dortzur/dlect",
  "author": "Dor Tzur",
  "license": "MIT",
  "peerDependencies": {
    "normalizr": "^3.2.4",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.0",
    "babel-preset-backpack": "^0.7.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "immer": "^1.3.0",
    "jest": "^22.4.3",
    "jest-junit": "^3.7.0",
    "json-loader": "^0.5.7",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.11.0"
  },
  "bugs": "https://github.com/dortzur/dlect/issues",
  "jest": {
    "testRegex": "(.spec.js)$"
  },
  "sideEffects": false,
  "dependencies": {
    "normalizr": "^3.2.4",
    "reselect": "^3.0.1"
  }
}
